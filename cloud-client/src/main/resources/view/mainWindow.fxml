            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="ru.gb.sklyarov.cloud.client.controller.MainController">
    <children>
        <TextField fx:id="commandTextField" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="477.0"
                   AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <Button layoutX="501.0" layoutY="14.0" mnemonicParsing="false" onAction="#sendCommand" prefHeight="25.0"
                prefWidth="93.0" text="Выполнить" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <TextArea fx:id="commandResultTextArea" editable="false" layoutX="14.0" layoutY="53.0" prefHeight="344.0"
                  prefWidth="580.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
                  AnchorPane.rightAnchor="10.0"/>
    </children>
</AnchorPane>
